{
  "name": "jike-sdk",
  "version": "0.13.1",
  "packageManager": "pnpm@6.32.6",
  "description": "Jike Ⓙ SDK for Node.js",
  "type": "module",
  "keywords": [
    "jike",
    "sdk"
  ],
  "homepage": "https://github.com/open-jike/jike-sdk#readme",
  "bugs": {
    "url": "https://github.com/open-jike/jike-sdk/issues"
  },
  "license": "AGPL-3.0",
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "module": "dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/node/index.cjs",
      "import": "./dist/node/index.js"
    },
    "./index": {
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-jike/jike-sdk.git"
  },
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "esmo playground/main.ts",
    "build": "pnpm run build:lib && pnpm run build:types",
    "build:lib": "MODE=modern tsup && MODE=node tsup",
    "build:types": "tsc --emitDeclarationOnly --declaration",
    "docs:build": "typedoc",
    "docs:preview": "serve docs",
    "lint": "eslint . --ext .js,.ts,.yml,.yaml,.md,.json,.json5",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest",
    "test:deno": "cd tests/deno && deno run --allow-net --import-map=import_map.json --allow-env index.ts",
    "release": "bumpp --commit --push --tag && npm publish",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@sxzz/eslint-config-prettier": "^2.1.1",
    "@sxzz/eslint-config-ts": "^2.1.1",
    "@types/md5": "^2.3.2",
    "@types/node": "*",
    "@types/sharp": "^0.30.2",
    "bumpp": "^7.1.1",
    "dayjs": "^1.11.0",
    "esbuild": "^0.14.36",
    "eslint": "^8.13.0",
    "eslint-define-config": "^1.3.0",
    "esno": "^0.14.1",
    "eventemitter3": "^4.0.7",
    "ky": "^0.30.0",
    "md5": "^2.3.0",
    "node-fetch": "3.2.3",
    "rollup-plugin-esbuild": "^4.9.1",
    "serve": "^13.0.2",
    "sharp": "^0.30.3",
    "tsup": "^5.12.5",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3",
    "vitest": "^0.9.3"
  },
  "engines": {
    "node": ">=14.17.0"
  }
}
